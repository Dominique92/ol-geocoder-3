{
  "name": "ol-geocoder",
  "description": "A geocoder extension for OpenLayers",
  "homepage": "https://github.com/Dominique92/ol-geocoder",
  "license": "MIT",
  "keywords": [
    "geocoder",
    "openlayers",
    "nominatim"
  ],
  "type": "module",
  "scripts": {
    "build": "rollup -c build/config.js",
    "lint": "npx eslint src/*.js && npx eslint src/*/*.js",
    "beautify": "js-beautify -s=2 build/*.js examples/* konstants/* src/*.*s src/helpers/* src/providers/* package.json"
  },
  "dependencies": {
    "ol": "8.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "latest",
    "@rollup/plugin-json": "latest",
    "@rollup/plugin-node-resolve": "latest",
    "@rollup/plugin-terser": "latest",
    "eslint": "latest",
    "js-beautify": "latest",
    "rollup": "latest",
    "rollup-plugin-import-css": "latest"
  }
}